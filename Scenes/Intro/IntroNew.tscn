code = "shader_type canvas_item;

void fragment() {
	vec4 col = texture(TEXTURE, UV);
	vec4 screenTex = texture(SCREEN_TEXTURE, SCREEN_UV);
	
	COLOR = vec4(1f - screenTex.r, 1f - screenTex.g, 1f - screenTex.b, col.a);
}
"

